<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.4" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>signin</servlet-name>
        <servlet-class>webProject.SigninServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>callback</servlet-name>
        <servlet-class>webProject.CallbackServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>webProject.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>post</servlet-name>
        <servlet-class>webProject.PostServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>quiz</servlet-name>
        <servlet-class>webProject.QuizServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signin</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>callback</servlet-name>
        <url-pattern>/callback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>post</servlet-name>
        <url-pattern>/post</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>quiz</servlet-name>
        <url-pattern>/quiz/sport</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>quiz</servlet-name>
        <url-pattern>/quiz/tv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>quiz</servlet-name>
        <url-pattern>/quiz/policy</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>quiz</servlet-name>
        <url-pattern>/quiz/game</url-pattern>
    </servlet-mapping>
    <error-page>
        <exception-type>twitter4j.TwitterException</exception-type>
        <location>/error.jsp</location>
    </error-page>
	<servlet>
		<servlet-name>SystemServiceServlet</servlet-name>
	  	<servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
	 	<init-param>
	   		<param-name>services</param-name>
	   		<param-value>webProject.TweetEntityEndpoint</param-value>
	   	</init-param>
	</servlet>
	<servlet-mapping>
	  	<servlet-name>SystemServiceServlet</servlet-name>
	  	<url-pattern>/_ah/spi/*</url-pattern>
	</servlet-mapping>
</web-app>